  "dependencies": {
    "express": "4.17.1",
    "mysql2": "3.0.0",
    "sequelize": "6.3.5"
  },
  "devDependencies": {
    "change-case": "4.1.2",
    "eslint-config-trybe-backend": "2.1.0",
    "jest": "29.3.1",
    "nodemon": "2.0.4",
    "sequelize-cli": "6.6.1",
